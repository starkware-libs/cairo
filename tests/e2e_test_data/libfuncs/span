//! > sierra_code
type felt = felt;
type Array<felt> = Array<felt>;
type Snapshot<Array<felt>> = Snapshot<Array<felt>>;
type u32 = u32;

libfunc array_len<felt> = array_len<felt>;
libfunc store_temp<u32> = store_temp<u32>;

array_len<felt>([0]) -> ([1]);
store_temp<u32>([1]) -> ([2]);
return([2]);

