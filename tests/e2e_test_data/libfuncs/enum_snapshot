    White: (),
    Black: (),
    Colorful: Array<felt252>,
}

fn foo(e: @Color) -> felt252 {
    match e {
        Color::White(_) => 0,
        Color::Black(_) => 1,
        Color::Colorful(_) => 2,
    }
}

