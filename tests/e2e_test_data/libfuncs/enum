//! > ==========================================================================

//! > match enum with more than 2 variants

//! > test_runner_name
SmallE2ETestRunner

//! > cairo
enum Color {
    Red: felt,
    Green: felt,
    Blue: felt,
}

fn foo(e: Color) -> felt {
    match e {
        Color::Red(_) => 0,
        Color::Green(_) => 1,
        Color::Blue(_) => 2,
    }
}

//! > casm
jmp rel [fp + -4];
jmp rel 8;
jmp rel 10;
[ap + 0] = 0, ap++;
jmp rel 10;
[ap + 0] = 1, ap++;
jmp rel 6;
[ap + 0] = 2, ap++;
jmp rel 2;
ret;

//! > function_costs
test::foo: OrderedHashMap({Const: 400})

//! > sierra_code
