store_temp<felt>([4]) -> ([5]);
jump() { 16() };
branch_align() -> ();
drop<felt>([2]) -> ();
felt_const<1>() -> ([6]);
store_temp<felt>([6]) -> ([5]);
jump() { 16() };
branch_align() -> ();
drop<felt>([3]) -> ();
felt_const<2>() -> ([7]);
store_temp<felt>([7]) -> ([5]);
jump() { 16() };
rename<felt>([5]) -> ([8]);
return([8]);

