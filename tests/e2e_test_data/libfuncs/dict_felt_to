type RangeCheck = RangeCheck;
type GasBuiltin = GasBuiltin;
type DictManager = DictManager;
type felt = felt;
type DictFeltTo<felt> = DictFeltTo<felt>;
type SquashedDictFeltTo<felt> = SquashedDictFeltTo<felt>;

