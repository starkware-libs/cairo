fn foo() -> felt {
    match from_nullable::<felt>(into_nullable::<felt>(into_box::<felt>(1234))) {
        FromNullableResult::Null(_) => 555,
        FromNullableResult::NotNull(x) => unbox::<felt>(x),
    }
}

