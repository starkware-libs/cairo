//! > sierra_code
type u16 = u16;
type u64 = u64;

libfunc upcast<u16, u64> = upcast<u16, u64>;
libfunc store_temp<u64> = store_temp<u64>;

upcast<u16, u64>([0]) -> ([1]);
store_temp<u64>([1]) -> ([2]);
return([2]);

