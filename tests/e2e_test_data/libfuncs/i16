//! > cairo
use zeroable::IsZeroResult;
use zeroable::NonZeroIntoImpl;
use traits::Into;
fn foo(a: i16) -> i16 {
    match integer::i16_is_zero(a) {
        IsZeroResult::Zero => 123_i16,
        IsZeroResult::NonZero(x) => x.into(),
    }
}

//! > casm
