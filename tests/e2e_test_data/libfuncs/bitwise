struct_construct<Tuple<u128, u128, u128>>([4], [5], [6]) -> ([7]);
store_temp<Bitwise>([3]) -> ([8]);
store_temp<Tuple<u128, u128, u128>>([7]) -> ([9]);
return([8], [9]);

