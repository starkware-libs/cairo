SmallE2ETestRunner

//! > cairo
use zeroable::IsZeroResult;
use zeroable::NonZeroIntoImpl;
use traits::Into;
fn foo(a: i32) -> i32 {
    match integer::i32_is_zero(a) {
        IsZeroResult::Zero(()) => 123_i32,
        IsZeroResult::NonZero(x) => x.into(),
    }
}

//! > casm
jmp rel 6 if [fp + -3] != 0;
[ap + 0] = 123, ap++;
jmp rel 3;
[ap + 0] = [fp + -3], ap++;
ret;

