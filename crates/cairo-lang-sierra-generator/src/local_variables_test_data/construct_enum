//! > function_code
fn foo() -> bool {
    let x = true;
    immovable(x);
    revoke_ap();
    x
}

//! > module_code
// Revokes ap since this function is recursive.
fn revoke_ap() -> felt252 {
    revoke_ap()
}

struct MyStruct {
    a: felt252,
    b: felt252,
}

#[inline(never)]
fn immovable<T>(t: T) -> T {
    t
}

