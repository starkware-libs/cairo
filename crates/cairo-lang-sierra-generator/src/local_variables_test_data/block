//! > Block

//! > test_runner_name
check_find_local_variables

//! > function_name
foo

//! > function_code
fn foo(_x: felt252) -> bool {
    let c = |a| {
        a + 3
    };
    return c(2_u32) == 5;
}

//! > module_code
// Revokes ap since this function is recursive.
fn revoke_ap() -> felt252 {
    revoke_ap()
}

