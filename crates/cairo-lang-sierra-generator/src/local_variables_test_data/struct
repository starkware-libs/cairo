//! > function_code
fn foo(x: felt) -> felt {
    let x2 = x + x;
    revoke_ap();
    let s = MyStruct{a: x, b: x2};
    let s2 = MyStruct{a: s.a, b: 0};
    let s3 = MyStruct{a: x, b: 1};
    revoke_ap();
    s3.a
}

//! > module_code
// Revokes ap since this function is recursive.
fn revoke_ap() -> felt {
    revoke_ap()
}

struct MyStruct {
    a: felt,
    b: felt,
}

impl MyStructDrop of Drop::<MyStruct>;

