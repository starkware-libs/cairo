//! > Match enum

//! > test_runner_name
check_find_local_variables

//! > function_name
foo

//! > function_code
fn foo() -> felt252 {
  let x = bar();
  let literal = 2;
  let w1 = non_literal();
  let y = match x {
    MyEnum::A(a) => {
      revoke_ap();
      a + a
    },
    MyEnum::B(b) => {
      // 'b' is not revoked.
      b + b + b
    }
  };
  // 'w1' is revoked because of the revoke_ap() inside the match.
  let w2 = w1 + w1;
  // 'literal' is not revoked since it is a const.
  let w3 = literal + literal;
  revoke_ap();
  // 'y' is revoked.
  y
}

//! > module_code
// Revokes ap since this function is recursive.
fn revoke_ap() -> felt252 {
    revoke_ap()
}

#[inline(never)]
fn non_literal() -> felt252 {
    1
}

fn bar() -> MyEnum {
    bar()
}

#[derive(Drop)]
enum MyEnum {
  A: felt252,
  B: felt252,
}

