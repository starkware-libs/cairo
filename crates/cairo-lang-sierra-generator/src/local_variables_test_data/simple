//! > test_runner_name
check_find_local_variables

//! > function_name
foo

//! > function_code
fn foo(x: felt) -> felt {
    let y = x + x;
    let z = y + y;
    let (z0, z1) = dup::<felt>(z);
    let (z2, z3) = dup::<felt>(z1);
    let (z4, z5) = dup::<felt>(z2);
    revoke_ap();
    z4 + z5
}

//! > module_code
// Revokes ap since this function is recursive.
fn revoke_ap() -> felt {
    revoke_ap()
}

