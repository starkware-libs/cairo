//! > function_code
fn foo(x: felt252) -> felt252 {
    let y0 = x + x;
    let y1 = y0 + y0;
    let y2 = y1 + y1;  // Note: y2 is not used below.
    revoke_ap();
    let z = x + y1;
    revoke_ap();
    let w = x + y1 + z;
    x
}

//! > module_code
// Revokes ap since this function is recursive.
fn revoke_ap() -> felt252 {
    revoke_ap()
}

