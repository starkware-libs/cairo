//! > function_code
fn foo(ref arr: Array<felt252>) {
    arr.append(5);
    immovable(arr.span());
    revoke_ap();
    arr.append(6);
}

//! > module_code
#[inline(never)]
fn immovable<T>(t: T) -> T { t }

// Revokes ap since this function is recursive.
fn revoke_ap() -> felt252 {
    revoke_ap()
}

