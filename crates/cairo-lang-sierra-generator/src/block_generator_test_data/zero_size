//! > Test calling a user function with zero sized parameters.

//! > test_runner_name
block_generator_test

//! > function
fn foo() {
    let (a, b) = get_val();
    test_func((), a, b)
}

//! > function_name
foo

//! > module_code
#[inline(never)]
fn get_val() -> (u32, ()) {
  (13, ())
}

fn test_func(empty: (), a: u32, b: ()) {}

