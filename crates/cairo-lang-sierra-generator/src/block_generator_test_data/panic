#[inline(always)]
fn expect(opt: Option<felt252>) -> felt252 {
    match opt {
        Some(x) => x,
        None => no_inline_panic(),
    }
}


#[inline(never)]
fn no_inline_panic() -> never {
    let mut data = array![];
    data.append(1);
    panic(data)
}

