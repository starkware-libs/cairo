//! > module_code
use array::ArrayTrait;

#[inline(always)]
fn expect(opt: Option<felt252>) -> felt252 {
   match opt {
      Option::Some(x) => x,
      Option::None(_) => no_inline_panic(),
  }
}


#[inline(never)]
fn no_inline_panic() -> never {
  let mut data = Default::default();
  data.append(1);
  panic(data)
}

