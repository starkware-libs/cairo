//! > Test variable lifetime for structs

//! > test_runner_name
check_variable_lifetime

//! > function_name
foo

//! > function_code
fn foo(x: MyStruct) -> MyStruct {
    let s = MyStruct{ a: x.a, b: x.b };
    let w = MyStruct{ a: s.a, b: s.b };
    MyStruct{ a: 10, b: 20 }
}

//! > module_code
#[derive(Copy)]
struct MyStruct {
    a: felt,
    b: felt,
}

impl MyStructDrop of Drop::<MyStruct>;

