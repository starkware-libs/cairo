//! > function
fn foo(myEnum: MyEnum) -> u8 {
    match myEnum {
        MyEnum::A(a) => a,
        MyEnum::B(b) => b,
        MyEnum::C((c1, c2)) => {
            if c1 > c2 {
                c1 + c2
            } else {
                c1 * c2
            }
        }
    }
}

