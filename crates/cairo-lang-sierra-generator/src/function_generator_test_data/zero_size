//! > Test calling a user function with zero sized parameters.

//! > test_runner_name
test_function_generator

//! > function
fn foo(a: u32) -> u32 {
    let a = push_builtin_and_val(a);
    test_func(Empty{}, a)
}

//! > function_name
foo

//! > module_code
#[derive(Drop)]
struct Empty {}

#[inline(never)]
fn push_builtin_and_val(a: u32) -> u32 {
  a | 13
}

fn test_func(empty: Empty, a: u32) -> u32 {
  a | 17
}

//! > semantic_diagnostics

//! > lowering_diagnostics

//! > sierra_gen_diagnostics

//! > sierra_code
label_test::foo::0:
store_temp<Bitwise>([0]) -> ([2])
store_temp<u32>([1]) -> ([3])
function_call<user@test::push_builtin_and_val>([2], [3]) -> ([4], [5])
struct_construct<test::Empty>() -> ([6])
rename<Bitwise>([4]) -> ([7])
rename<u32>([5]) -> ([8])
function_call<user@test::test_func>([7], [6], [8]) -> ([9], [10])
rename<Bitwise>([9]) -> ([11])
rename<u32>([10]) -> ([12])
return([11], [12])
