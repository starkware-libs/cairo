//! > test goto + fallthrough convergence.

//! > test_runner_name
test_function_generator

//! > function
fn foo(a: Array::<felt>) -> @felt {
    let t: Option::<@felt> = a.get(0_u32);
    inlined_expect(t)
}

//! > function_name
foo

//! > module_code
use array::ArrayTrait;

#[inline(always)]
fn inlined_expect(option: Option::<@felt>) -> @felt {
    match option {
        Option::Some(x) => x,
        Option::None(()) => @0,
    }
}

