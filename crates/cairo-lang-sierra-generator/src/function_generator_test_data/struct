unbox<Snapshot<test::A>>([1]) -> ([2])
drop<Snapshot<test::A>>([2]) -> ()
struct_boxed_deconstruct<Snapshot<test::A>>([0]) -> ([3], [4])
drop<Box<Snapshot<Array<felt252>>>>([4]) -> ()
store_temp<Box<Snapshot<Array<felt252>>>>([3]) -> ([3])
