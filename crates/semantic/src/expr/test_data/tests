//! > Test unexpected argument types for operators.

//! > Function
func foo(a: MyType) {
    a + a * a
}

//! > Function Name
foo

//! > Module Code
extern type MyType;

//! > Expected Result
error: Unexpected argument type. Expected: "core::felt", found: "test_crate::MyType".
 --> lib.cairo:3:9
    a + a * a
        ^

error: Unexpected argument type. Expected: "core::felt", found: "test_crate::MyType".
 --> lib.cairo:3:13
    a + a * a
            ^

error: Unexpected argument type. Expected: "core::felt", found: "test_crate::MyType".
 --> lib.cairo:3:5
    a + a * a
    ^

//! > Test operators.

//! > Function
func foo(a: felt, b: bool) {
    // Valid.
    a + a * a - a;
    b && b || b && (a == a || a > a)
    && (a<a) && (a<=a) && (a>=a);
    // Errors.
    a>a>a;
}

//! > Function Name
foo

//! > Module Code

//! > Expected Result
error: Unexpected argument type. Expected: "core::felt", found: "core::bool".
 --> lib.cairo:7:5
    a>a>a;
    ^*^
