//! > Test enum

//! > test_function_name
test_function_diagnostics

//! > function
enum A {
    a: (),
    b: felt,
}

func foo() {
    let a : A = A::a(());
    let a : A = A::b(5);
    let b : bool = bool::True(());
    let b : bool = true;

    // Failures.
    let a : A = A::a();
    let a : A = A::b(1,2);
    let a : A = A::a::a(());
    let a : A = A::c();
    let a : A = A::d;
    let a : A = A;
}

//! > function_name
foo

//! > module_code

//! > expected_diagnostics
