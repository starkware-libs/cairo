//! > Test basic error propagation on Option.

//! > test_function_name
test_function_diagnostics

//! > function
func foo() -> Option::<felt> {
    with_err()?;
    Option::<felt>::None(())
}

//! > function_name
foo

//! > module_code
func with_err() -> Option::<felt> {
    Option::<felt>::None(())
}

//! > expected_diagnostics

//! > ==========================================================================

//! > Test bad function for error propagation on Option.

//! > test_function_name
test_function_diagnostics

//! > function
func foo() -> felt {
    with_err()?;
    1
}

//! > function_name
foo

//! > module_code
func with_err() -> Option::<felt> {
    Option::<felt>::None(())
}

//! > expected_diagnostics
error: Return type "core::felt" does not wrap error "()"
 --> lib.cairo:5:5
    with_err()?;
    ^*********^

//! > ==========================================================================

//! > Test basic error propagation on Result.

//! > test_function_name
test_function_diagnostics

//! > function
func foo() -> Result::<felt, u128> {
    with_err()?;
    Result::<felt, u128>::Ok(with_err2()?)
}

//! > function_name
foo

//! > module_code
func with_err() -> Result::<(), u128> {
    Result::<(), u128>::Ok(())
}

func with_err2() -> Result::<felt, u128> {
    Result::<felt, u128>::Ok((0))
}

//! > expected_diagnostics

//! > ==========================================================================

//! > Test bad function for error propagation on Result.

//! > test_function_name
test_function_diagnostics

//! > function
func foo() -> felt {
    with_err()?;
    1
}

//! > function_name
foo

//! > module_code
func with_err() -> Result::<felt, u128> {
    Result::<felt, u128>::Ok((0))
}

//! > expected_diagnostics
error: Return type "core::felt" does not wrap error "core::integer::u128"
 --> lib.cairo:5:5
    with_err()?;
    ^*********^

//! > ==========================================================================

//! > Test error propagation on mismatching Result types.

//! > test_function_name
test_function_diagnostics

//! > function
func foo() -> Result::<felt, u128> {
    with_other_err()?;
    Result::<felt, felt>::Ok((0))
}

//! > function_name
foo

//! > module_code
func with_other_err() -> Result::<felt, felt> {
    Result::<felt, felt>::Ok((0))
}

//! > expected_diagnostics
