//! > module_code
struct A {}
trait MyTrait<T> {
    func param_test(a: uint128) -> felt;

    func no_ret_ty(a: uint128) -> felt;
}
impl MyImpl of MyTrait::<A>;
impl MyImpl2<T> of MyTrait::<T> {
    mod a;
    use b;
    extern func extern_func();
    extern type DummyType;

    trait DummyTrait;
    impl DummyImpl of MyTrait::<T>;

    struct DummyStruct {}

    enum DummyEnum {}

    func my_bar() {

    }

    func param_test(a: felt, b: felt) -> uint128 {

    }

    func no_ret_ty(a: uint128) {

    }
}

