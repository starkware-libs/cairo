//! > Test trivial pow2 by add loop

//! > test_runner_name
test_profiling

//! > collect_scoped_sierra_statement_weights
true

//! > scoped_mode
true

//! > cairo_code
fn pow2_14000() {
    pow2_by_add_loop(14000, 1);
}

fn pow2_by_add_loop(n: felt252, x: felt252) -> felt252 {
    if n == 0 {
        return x;
    } else {
        return pow2_by_add_loop(n - 1, x + x);
    }
}

