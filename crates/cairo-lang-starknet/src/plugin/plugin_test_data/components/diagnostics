//! > cairo_code
#[starknet::component]
mod component {
    use super::MyTrait;

    #[storage]
    struct Storage {}

    #[embeddable_as]
    impl MyInnerImpl<
        TContractState, impl X: HasComponent<TContractState>
    > of MyTrait<ComponentState<TContractState>> {
        fn do_nothing(self: @ComponentState<TContractState>) {}
    }
}
