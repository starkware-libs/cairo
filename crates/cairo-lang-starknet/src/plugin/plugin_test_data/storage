#[derive(Drop)]
    struct ContractState {
        var_felt252: var_felt252::ContractState,
        var_u8: var_u8::ContractState,
        var_u16: var_u16::ContractState,
        var_u32: var_u32::ContractState,
        var_u64: var_u64::ContractState,
