        let idx = starknet::Store::<felt252>::read_at_offset(address_domain, base, offset)?;
        if idx == 1 {
            starknet::SyscallResult::Ok(
                EnumWithDefault::A(
                    starknet::Store::read_at_offset(address_domain, base, offset + 1_u8)?
                )
            )
        }
