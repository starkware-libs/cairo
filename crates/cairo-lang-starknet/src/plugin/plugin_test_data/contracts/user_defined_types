            else if idx == 1 { BadEnumWithDefault::B(core::serde::Serde::deserialize(ref serialized)?) }
            else if idx == 2 { BadEnumWithDefault::C(core::serde::Serde::deserialize(ref serialized)?) }
            else { return core::option::Option::None; }
        )
    }
}


