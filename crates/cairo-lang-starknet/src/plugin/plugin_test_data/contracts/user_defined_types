            else if idx == 1 { EnumWithDefault::B(serde::Serde::deserialize(ref serialized)?) }
            else if idx == 2 { EnumWithDefault::C(serde::Serde::deserialize(ref serialized)?) }
            else { return Option::None; }
        )
    }
}


