    struct Storage {
        var: WrappedFelt252,
        zero_size: ZeroSize,
        mapping: LegacyMap::<WrappedFelt252, WrappedFelt252>,
        zero_size_mapping: LegacyMap::<ZeroSize, ZeroSize>,
    }
