    impl ContractStateEventEmitter of starknet::event::EventEmitter<
        ContractState, Event
    > {
        fn emit<S, impl IntoImp: core::traits::Into<S, Event>>(
            ref self: ContractState, event: S
        ) {
            let event: Event = core::traits::Into::into(event);
            let mut keys = Default::<core::array::Array>::default();
            let mut data = Default::<core::array::Array>::default();
            starknet::Event::append_keys_and_data(@event, ref keys, ref data);
            starknet::SyscallResultTrait::unwrap_syscall(
                starknet::syscalls::emit_event_syscall(
                    core::array::ArrayTrait::span(@keys),
                    core::array::ArrayTrait::span(@data),
                )
            )
        }
    }
impl EventDrop of core::traits::Drop::<Event>;
impl EventIsEvent of starknet::Event<Event> {
    fn append_keys_and_data(
        self: @Event, ref keys: Array<felt252>, ref data: Array<felt252>
    ) {
        match self {
            Event::Transfer(val) => {
                core::array::ArrayTrait::append(ref keys, selector!("Transfer"));
                starknet::Event::append_keys_and_data(
                    val, ref keys, ref data
                );
            },
            Event::Approval(val) => {
                core::array::ArrayTrait::append(ref keys, selector!("Approval"));
                starknet::Event::append_keys_and_data(
                    val, ref keys, ref data
                );
            },
        }
    }
    fn deserialize(
        ref keys: Span<felt252>, ref data: Span<felt252>,
    ) -> Option<Event> {
        let __selector__ = *core::array::SpanTrait::pop_front(ref keys)?;
        if __selector__ == selector!("Transfer") {
                let val = starknet::Event::deserialize(
                    ref keys, ref data
                )?;
                return Option::Some(Event::Transfer(val));
        }
        if __selector__ == selector!("Approval") {
                let val = starknet::Event::deserialize(
                    ref keys, ref data
                )?;
                return Option::Some(Event::Approval(val));
        }
        Option::None
    }
}
impl EventTransferIntoEvent of Into<Transfer, Event> {
    fn into(self: Transfer) -> Event {
        Event::Transfer(self)
    }
}
impl EventApprovalIntoEvent of Into<Approval, Event> {
    fn into(self: Approval) -> Event {
        Event::Approval(self)
    }
}
impl TransferDrop of core::traits::Drop::<Transfer>;
impl TransferIsEvent of starknet::Event<Transfer> {
    fn append_keys_and_data(
        self: @Transfer, ref keys: Array<felt252>, ref data: Array<felt252>
    ) {
            core::serde::Serde::serialize(self.from, ref data);
            core::serde::Serde::serialize(self.to, ref data);
            core::serde::Serde::serialize(self.value, ref data);
    }
    fn deserialize(
        ref keys: Span<felt252>, ref data: Span<felt252>,
    ) -> Option<Transfer> {
                let from = core::serde::Serde::deserialize(
                    ref data
                )?;
                let to = core::serde::Serde::deserialize(
                    ref data
                )?;
                let value = core::serde::Serde::deserialize(
                    ref data
                )?;
        Option::Some(Transfer {from, to, value, })
    }
}
impl ApprovalDrop of core::traits::Drop::<Approval>;
impl ApprovalIsEvent of starknet::Event<Approval> {
    fn append_keys_and_data(
        self: @Approval, ref keys: Array<felt252>, ref data: Array<felt252>
    ) {
            core::serde::Serde::serialize(self.owner, ref data);
            core::serde::Serde::serialize(self.spender, ref data);
            core::serde::Serde::serialize(self.value, ref data);
    }
    fn deserialize(
        ref keys: Span<felt252>, ref data: Span<felt252>,
    ) -> Option<Approval> {
                let owner = core::serde::Serde::deserialize(
                    ref data
                )?;
                let spender = core::serde::Serde::deserialize(
                    ref data
                )?;
                let value = core::serde::Serde::deserialize(
                    ref data
                )?;
        Option::Some(Approval {owner, spender, value, })
    }
}
    trait StorageTrait {
        fn transfer_helper(
            ref self: ContractState,
            sender: ContractAddress,
            recipient: ContractAddress,
            amount: u256
        );

        fn spend_allowance(
            ref self: ContractState, owner: ContractAddress, spender: ContractAddress, amount: u256
        );

        fn approve_helper(
            ref self: ContractState, owner: ContractAddress, spender: ContractAddress, amount: u256
        );
    }
impl ContractStorageBaseDrop of core::traits::Drop::<ContractStorageBase>;
impl ContractStorageBaseCopy of core::traits::Copy::<ContractStorageBase>;
impl ContractStorageBaseMutDrop of core::traits::Drop::<ContractStorageBaseMut>;
impl ContractStorageBaseMutCopy of core::traits::Copy::<ContractStorageBaseMut>;
}
trait IERC20DispatcherTrait<T> {
    fn get_name(self: T) -> felt252;
    fn get_symbol(self: T) -> felt252;
    fn get_decimals(self: T) -> u8;
    fn get_total_supply(self: T) -> u256;
    fn balance_of(self: T, account: ContractAddress) -> u256;
    fn allowance(self: T, owner: ContractAddress, spender: ContractAddress) -> u256;
    fn transfer(self: T, recipient: ContractAddress, amount: u256);
    fn transfer_from(
self: T, sender: ContractAddress, recipient: ContractAddress, amount: u256
    );
    fn approve(self: T, spender: ContractAddress, amount: u256);
    fn increase_allowance(self: T, spender: ContractAddress, added_value: u256);
    fn decrease_allowance(
self: T, spender: ContractAddress, subtracted_value: u256
    );
    fn write_info(self: T, user_info: UserInfo);
    fn get_info(self: T) -> UserInfo;
}

