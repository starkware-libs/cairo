        let HelloStarknetTraitDispatcher {
            contract_address,
        } = value;
        starknet::Store::<starknet::ContractAddress>::write(__store_derive_address_domain__, __store_derive_base__, contract_address)?;
        starknet::SyscallResult::Ok(())
    }
