//! > cairo_code
#[derive(starknet::Event, starknet::Store, PartialEq, Drop, Serde)]
struct A {
    x: felt252,
    #[key]
    data: usize,
}

#[derive(starknet::Event, PartialEq, Drop, Serde)]
struct B {
    x: felt252,
}


#[derive(starknet::Event, PartialEq, Drop)]
enum NestedEventEnum {
    B: B,
}


#[derive(starknet::Event, PartialEq, Drop)]
enum MyEventEnum {
    A: A,
    B: B,
    #[flat]
    C: NestedEventEnum,
    #[flat]
    D: NestedEventEnum,
}

