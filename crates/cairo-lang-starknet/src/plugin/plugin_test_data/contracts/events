impl NestedEventEnumPartialEq of core::traits::PartialEq::<NestedEventEnum> {
    fn eq(lhs: @NestedEventEnum, rhs: @NestedEventEnum) -> bool {
        match lhs {
            NestedEventEnum::B(x) => match rhs {
                NestedEventEnum::B(y) => x == y,
            },
        }
    }
}
