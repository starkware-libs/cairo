impl StoreA of starknet::Store::<A> {
    fn read(address_domain: u32, base: starknet::storage_access::StorageBaseAddress) -> starknet::SyscallResult<A> {
        let __store_derive_address_domain__ = address_domain;
        let __store_derive_base__ = base;
        let x = starknet::Store::<felt252>::read(__store_derive_address_domain__, __store_derive_base__)?;
        let mut __store_derive_current_offset__ = starknet::Store::<felt252>::size();
        let data = starknet::Store::<usize>::read_at_offset(__store_derive_address_domain__, __store_derive_base__, __store_derive_current_offset__)?;
        starknet::SyscallResult::Ok(
            A {
                x,
                data,
            }
        )
    }
    fn write(address_domain: u32, base: starknet::storage_access::StorageBaseAddress, value: A) -> starknet::SyscallResult<()> {
        let __store_derive_address_domain__ = address_domain;
        let __store_derive_base__ = base;
        let A {
            x,
                data,
        } = value;
        starknet::Store::<felt252>::write(__store_derive_address_domain__, __store_derive_base__, x)?;
