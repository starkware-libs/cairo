//! > cairo_code
#[starknet::contract]
mod test_contract {
    #[storage]
    struct Storage {}

    #[l1_handler]
    fn good_l1_handler(ref self: ContractState, from_address: felt252, arg: felt252) {}

    #[l1_handler]
    fn good_l1_handler_ignored(ref self: ContractState, _from_address: felt252, arg: felt252) {}
}

