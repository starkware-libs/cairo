    struct Storage {
        #[nested(v0)]
        non_path_type: (usize, usize),
        #[nested(v0)]
        non_component: usize,
        // non-v0 nested member.
        #[nested]
        test_component_storage: super::test_component::Storage,
    }

    #[event]
    #[derive(Drop, starknet::Event)]
    enum Event {
        ABC: super::test_component::Event,
    }
