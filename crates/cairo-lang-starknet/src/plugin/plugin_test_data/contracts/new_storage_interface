}
impl BalancePairStorageNodeImpl of starknet::storage::StorageNode<BalancePair> {
    type NodeType = BalancePairStorageNode;
    fn storage_node(self: starknet::storage::StoragePath<BalancePair>) -> BalancePairStorageNode {
