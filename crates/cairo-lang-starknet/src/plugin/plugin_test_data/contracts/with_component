    pub struct ContractState {
        pub test_component_storage: super::test_component::ComponentState<ContractState>,
        pub normal_member1: __member_module_normal_member1::ContractMemberState,
        pub normal_member2: __member_module_normal_member2::ContractMemberState,
    }
    impl ContractStateDrop of Drop<ContractState> {}
    #[inline(always)]
    pub fn unsafe_new_contract_state() -> ContractState {
        ContractState {
            test_component_storage: super::test_component::unsafe_new_component_state::<ContractState>(),
            normal_member1: __member_module_normal_member1::ContractMemberState {},
            normal_member2: __member_module_normal_member2::ContractMemberState {},
        }
    }
    #[cfg(test)]
    #[inline(always)]
