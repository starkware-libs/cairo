#[starknet::embeddable]
#[generate_trait]
impl OutsideImpl<TContractState> of OutsideTrait<TContractState> {
    fn ret_3(self: @TContractState) -> felt252 {
        3
    }
    fn ret_identity(ref self: TContractState, from_address: felt252, value: felt252) -> felt252 {
        value
    }
}

