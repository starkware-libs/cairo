//! > Test expansion of a dispatcher.

//! > test_runner_name
ExpandContractTestRunner

//! > cairo_code
#[abi]
trait IContract {
    fn get_something(arg: felt, num: felt) -> felt;

    fn empty();

    fn bad_sig(ref arg1: felt, ref arg2: felt) -> felt;

    #[event]
    fn my_event();
}

//! > generated_cairo_code
#[abi]
trait IContract {
    fn get_something(arg: felt, num: felt) -> felt;

    fn empty();

    fn bad_sig(ref arg1: felt, ref arg2: felt) -> felt;

    #[event]
    fn my_event();
}
#[derive(Copy, Drop)]
struct IContractDispatcher {
    contract_address: starknet::ContractAddress,
}

trait IContractDispatcherTrait {
    fn get_something(self: IContractDispatcher, arg: felt, num: felt) -> felt;
    fn empty(self: IContractDispatcher, );
}

impl IContractDispatcherImpl of IContractDispatcherTrait {
    fn get_something(self: IContractDispatcher, arg: felt, num: felt) -> felt {
        let entry_point_selector = 0x3c52d61651de3dcab6ceaa9f6505f7aed8f1ffc0f694ce2a9ed76e758d87a3;
        let mut calldata = array::ArrayTrait::new();
        serde::Serde::<felt>::serialize(ref calldata, arg);
        serde::Serde::<felt>::serialize(ref calldata, num);

        let mut ret_data = array::ArrayTrait::span(
            @starknet::SyscallResultTrait::unwrap_syscall(
                starknet::call_contract_syscall(
                    self.contract_address,
                    entry_point_selector,
                    calldata,
                )
            )
        );

        option::OptionTrait::expect(
            serde::Serde::<felt>::deserialize(ref ret_data),
            'Returned data too short',
        )
    }

    fn empty(self: IContractDispatcher, ) {
        let entry_point_selector = 0x1fc3f77ebc090777f567969ad9823cf6334ab888acb385ca72668ec5adbde80;
        let mut calldata = array::ArrayTrait::new();

        let mut ret_data = array::ArrayTrait::span(
            @starknet::SyscallResultTrait::unwrap_syscall(
                starknet::call_contract_syscall(
                    self.contract_address,
                    entry_point_selector,
                    calldata,
                )
            )
        );

    }

}

