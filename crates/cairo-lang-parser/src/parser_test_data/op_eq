//! > Test +=, -=, *=, /=, %=

//! > test_runner_name
test_partial_parser_tree

//! > cairo_code
fn foo() {
    let mut x = 3;
    x += 5;
    x -= 5;
    x *= 5;
    x /= 5;
    x %= 5;
    x = 5;
}

//! > top_level_kind
StatementList

//! > ignored_kinds
StatementLet

//! > expected_diagnostics

//! > expected_tree
