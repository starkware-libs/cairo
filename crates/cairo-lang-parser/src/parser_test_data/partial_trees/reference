//! > ==========================================================================

//! > Test double reference with literal &&5_u32

//! > test_runner_name
test_partial_parser_tree(expect_diagnostics: false)

//! > cairo_code
fn f() {
    let x = &1_u32;
    let y = &&2_u32;
    let z = &&&3_u32;
}

//! > top_level_kind
ExprUnary

//! > ignored_kinds

//! > expected_diagnostics

//! > expected_tree
└── Top level kind: ExprUnary
    ├── op (kind: TokenAnd): '&'
    └── expr (kind: TokenLiteralNumber): '1_u32'
└── Top level kind: ExprUnary
    ├── op (kind: TokenAnd): '&'
    └── expr (kind: ExprUnary)
        ├── op (kind: TokenAnd): '&'
        └── expr (kind: TokenLiteralNumber): '2_u32'
└── Top level kind: ExprUnary
    ├── op (kind: TokenAnd): '&'
    └── expr (kind: ExprUnary)
        ├── op (kind: TokenAnd): '&'
        └── expr (kind: ExprUnary)
            ├── op (kind: TokenAnd): '&'
            └── expr (kind: TokenLiteralNumber): '3_u32'
