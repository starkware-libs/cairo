//! > Test let-else

//! > test_runner_name
get_diagnostics

//! > cairo_code
fn f() {
    { let Some(x) = 5 }
    { let Some(x) = 5 else }
    { let Some(x) = 5 else { } }
    { let Some(x) = 5 else 0; }
}

