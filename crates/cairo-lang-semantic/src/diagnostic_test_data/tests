//! > expr_code
{
  let x = Struct {};
  MyTrait::deprecated_method(@x);
}

//! > module_code
pub struct Struct {}

pub trait MyTrait {
    #[deprecated(feature: "deprecated", note: "This method is deprecated", since: "1.0.0")]
    fn deprecated_method(self: @Struct);
}

impl MyTraitImpl of MyTrait {
    fn deprecated_method(self: @Struct) {}
}

