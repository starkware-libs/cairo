//! > module_code
pub trait NegImpl {
}

pub trait Bad<T, -NegImpl> {
    fn bad() -> T;
}

pub impl ImplBad<T, -NegImpl> of Bad<T, _>{
    fn bad() -> T {
        panic!("Bad::bad() called")
    }
}

pub impl ImplBad2<T> of Bad<T, T>{
    fn bad() -> T {
        panic!("Bad::bad() called")
    }
}

