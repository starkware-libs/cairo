//! > ==========================================================================

//! > Cannot assign to mutable variable after referencing it.

//! > test_runner_name
test_function_diagnostics(expect_diagnostics: true)

//! > function
fn foo(mut param: u32) {
    let mut x = 3_u32;
    let _r = &x;
    x = 5;

    let mut y = 2_felt252;
    let _s = &y;
    y = 10;
}

//! > function_name
foo

//! > module_code

//! > expected_diagnostics
error: Cannot assign to a variable with a taken pointer
 --> lib.cairo:4:5
    x = 5;
    ^^^^^
note: variable pointer taken here:
  --> lib.cairo:3:14
    let _r = &x;
             ^^

error: Cannot assign to a variable with a taken pointer
 --> lib.cairo:8:5
    y = 10;
    ^^^^^^
note: variable pointer taken here:
  --> lib.cairo:7:14
    let _s = &y;
             ^^
