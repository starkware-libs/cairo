//! > module_code
trait OtherTrait<T> {
    const X: u32;
}
impl OtherImpl of OtherTrait<u32> {
    const X: u32 = 4;
}

trait MyTrait {
    type ty;
    impl Other: OtherTrait<Self::ty>;
}
impl MyImpl of MyTrait {
    type ty = u32;
    impl Other = OtherImpl;
}

