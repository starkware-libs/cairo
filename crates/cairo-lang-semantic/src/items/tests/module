//! > Test redefinition of the same name.

//! > test_runner_name
test_function_diagnostics

//! > function
fn foo() {}

//! > function_name
foo

//! > module_code
fn abc() {}

fn abc(a : felt252) {}

mod inner {
   struct abc {}
}
use inner::abc;

struct abc {
   a: felt252,
}

enum abc {
   Abc: felt252,
}

mod abc {}

trait abc {}

impl abc of abc;

//! > expected_diagnostics
