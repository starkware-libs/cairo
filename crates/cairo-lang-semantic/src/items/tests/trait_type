//! > module_code
trait OtherAdd<Lhs, Rhs> {
    type Output;
    fn other_add(lhs: Lhs, rhs: Rhs) -> Self::Output;
}
impl DepAdd<T> of OtherAdd<T, T> {
    type Output = T;
    fn other_add(lhs: T, rhs: T) -> T {
        lhs
    }
}

