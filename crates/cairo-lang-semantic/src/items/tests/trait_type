    fn bar1(x: AnotherTrait::AnotherType);
    fn bar2() -> AnotherTrait::AnotherType;
}
impl MyImpl of MyTrait {
    fn bar1(x: u32) {}
    fn bar2() -> u32 {
        3_u32
    }
}

