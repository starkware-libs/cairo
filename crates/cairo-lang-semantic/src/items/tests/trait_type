//! > module_code
trait FirstTrait<T> {
    type Element;
    fn first(self: T) -> Self::Element;
}

impl ArrayFirst<T> of FirstTrait<Array<T>> {
    type Element = T;
    fn first(mut self: Array<T>) -> T {
        self.pop_front().unwrap()
    }
}

