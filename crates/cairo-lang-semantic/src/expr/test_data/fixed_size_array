//! > Test fixed size array.

//! > test_runner_name
test_function_diagnostics(expect_diagnostics: false)

//! > function
fn foo() {
    let _x: [u32; 3]  = [1; 3];
    let _y: [u32; 3]  = [1, 2, 3];
    let _z  = [1, 2, 3];
}

//! > function_name
foo

//! > module_code

//! > expected_diagnostics

//! > ==========================================================================

//! > Test fixed size array incompatible size.

//! > test_runner_name
test_function_diagnostics(expect_diagnostics: true)

//! > function
fn foo() {
    let _x: [u32; 2]  = [1, 2, 3];
    let _x: [u32; 2]  = [1; 3];
}

//! > function_name
foo

//! > module_code

//! > expected_diagnostics
error: Unexpected argument type. Expected: "[core::integer::u32; 2]", found: "[?2; 3]".
 --> lib.cairo:2:25
    let _x: [u32; 2]  = [1, 2, 3];
                        ^*******^

error: Unexpected argument type. Expected: "[core::integer::u32; 2]", found: "[?3; 3]".
 --> lib.cairo:3:25
    let _x: [u32; 2]  = [1; 3];
                        ^****^

//! > ==========================================================================

//! > Test fixed size array illegal number of elements.

//! > test_runner_name
test_function_diagnostics(expect_diagnostics: true)

//! > function
fn foo() {
    let _x: [u32, u32; 2]  = [1, 2];
    let _x: [; 2]  = [1, 2];
    let _x: [u32; 2]  = [1, 2; 2];
    let _x: [u32; 2]  = [];
    let _x: [u32; 2]  = [; 2];
}

//! > function_name
foo

//! > module_code

//! > expected_diagnostics
error: Fixed size array type must have exactly one type.
 --> lib.cairo:2:13
    let _x: [u32, u32; 2]  = [1, 2];
            ^***********^

error: Fixed size array type must have exactly one type.
 --> lib.cairo:3:13
    let _x: [; 2]  = [1, 2];
            ^***^

error: Fixed size array with defined size must have exactly one value.
 --> lib.cairo:4:25
    let _x: [u32; 2]  = [1, 2; 2];
                        ^*******^

error: Fixed size array must have at least one element.
 --> lib.cairo:5:25
    let _x: [u32; 2]  = [];
                        ^^

error: Fixed size array must have at least one element.
 --> lib.cairo:6:25
    let _x: [u32; 2]  = [; 2];
                        ^***^

//! > ==========================================================================

//! > Test fixed size array illegal size.

//! > test_runner_name
test_function_diagnostics(expect_diagnostics: true)

//! > function
fn foo() {
    let _x: [u32; -1]  = [1; -1];
    let _x: [u32; SIZE]  = [1; SIZE];
    let _x: [u32; '1']  = [1; '1'];
}

//! > function_name
foo

//! > module_code
const SIZE: usize = 2;

//! > expected_diagnostics
error: Fixed size array type must have a positive integer size.
 --> lib.cairo:3:30
