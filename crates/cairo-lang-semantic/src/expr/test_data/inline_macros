    ($x:ident) => {
        1
    };
}

//! > expected_diagnostics

//! > ==========================================================================

//! > Test user defined macro overwriting plugin macro.

//! > test_runner_name
test_function_diagnostics(expect_diagnostics: false)

//! > function
fn foo() {
    let x = 1;
    let y = 1;
    assert_eq!(x, y);
}

//! > function_name
foo

//! > module_code
macro assert_eq {
    ($x:ident, $y:ident) => {
        if $x != $y {
            $defsite::panic!("Panic!");
        }
    };
}

//! > expected_diagnostics

