    let _a: A = A::a();
                ^^^^^^

error[E2030]: Wrong number of arguments. Expected 1, found: 2
 --> lib.cairo:14:17
    let _a: A = A::b(1, 2);
                ^^^^^^^^^^

error[E2086]: Invalid path.
 --> lib.cairo:15:23
    let _a: A = A::a::a(());
                      ^

error[E2060]: Enum "test::A" has no variant "c"
 --> lib.cairo:16:20
    let _a: A = A::c();
                   ^

error[E2060]: Enum "test::A" has no variant "d"
 --> lib.cairo:17:20
    let _a: A = A::d;
                   ^

