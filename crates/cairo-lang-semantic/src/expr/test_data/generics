 --> lib.cairo:6:32
    let bad : A::<A::<bool>> = res;
                               ^*^

//! > ==========================================================================

//! > Test generics in trait functions.

//! > test_function_name
test_function_diagnostics

//! > function
fn foo() {
    5.foo(true);
    MyTrait::foo(6, false);
}

//! > module_code
trait MyTrait<T> {
    fn foo<S>(self: T, y: S) -> (T, S);
}
impl MyImpl of MyTrait::<felt> {
    fn foo<S>(self: felt, y: S) -> (felt, S) {
        (self, y)
    }
}

//! > function_name
foo

//! > expected_diagnostics
