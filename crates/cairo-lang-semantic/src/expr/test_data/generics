//! > function
fn foo() {
    let mut x = bar();
    x = bar::<felt252, _>();
    x = bar::<_, u8>();
    x = bar::<u16, u32>();
}

//! > module_code
struct MyStruct<S, T> {}

extern fn bar<S, T>() -> MyStruct<S, T> nopanic;

