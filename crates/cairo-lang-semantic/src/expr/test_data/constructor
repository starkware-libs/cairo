//! > Test constructor with variables that don't exist.

//! > test_runner_name
test_function_diagnostics

//! > function_code
fn foo() {
    let _x = MyStruct { a, b, a: 3 };
}

//! > function_name
foo

//! > module_code
struct MyStruct {
    a: usize,
    b: usize,
}

//! > expected_diagnostics
