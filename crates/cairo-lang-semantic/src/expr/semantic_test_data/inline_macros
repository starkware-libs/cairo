//! > Test types for array macro with method call

//! > test_runner_name
test_expr_semantics(expect_diagnostics: false)

//! > module_code

//! > expr_code
array![10, 11, 12].span()

//! > expected_semantics
FunctionCall(
    ExprFunctionCall {
        function: core::array::ArrayImpl::<core::felt252>::span,
        args: [
            Value(
                Snapshot(
                    ExprSnapshot {
                        inner: Block(
                            ExprBlock {
                                statements: [
                                    Let(
                                        StatementLet {
                                            pattern: Variable(
