//! > expected_diagnostics

//! > ==========================================================================

//! > Test ref parameter after reference taken

//! > test_runner_name
test_expr_semantics

//! > module_code
fn modify_value(ref x: u32) {
    x = x + 1;
}

//! > function_body
let mut value = 5_u32;
// Take a reference - this should prevent mutation through ref parameter.
let r = &value;

//! > expr_code
modify_value(ref value)


//! > expected_semantics
FunctionCall(
    ExprFunctionCall {
        function: test::modify_value,
        args: [
            Reference(
                LocalVarId(test::value),
            ),
        ],
        coupon_arg: None,
        ty: (),
    },
)

//! > expected_diagnostics
error: Cannot assign to a variable with a taken pointer
 --> lib.cairo:7:18
modify_value(ref value)
                 ^^^^^
note: variable pointer taken here:
  --> lib.cairo:6:9
let r = &value; {
        ^^^^^^

warning[E0001]: Unused variable. Consider ignoring by prefixing with `_`.
 --> lib.cairo:6:5
let r = &value; {
    ^
