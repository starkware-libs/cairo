//! > ==========================================================================

//! > Test bitwise AND with reference operator

//! > test_runner_name
test_expr_semantics

//! > function_body
let a = &3_u32;
let b = 5_u32;

//! > expr_code
a & &b


//! > expected_semantics
FunctionCall(
    ExprFunctionCall {
        function: ?1::bitand,
        args: [
            Value(
                Var(
                    LocalVarId(test::a),
                ),
            ),
            Value(
                FunctionCall(
                    ExprFunctionCall {
                        function: core::box::BoxImpl::<@core::integer::u32>::new,
                        args: [
                            Value(
                                Snapshot(
                                    ExprSnapshot {
                                        inner: Var(
                                            LocalVarId(test::b),
                                        ),
                                        ty: @core::integer::u32,
                                    },
                                ),
                            ),
                        ],
                        coupon_arg: None,
                        ty: core::box::Box::<@core::integer::u32>,
                    },
                ),
            ),
        ],
        coupon_arg: None,
        ty: core::box::Box::<@core::integer::u32>,
    },
)

//! > expected_diagnostics
