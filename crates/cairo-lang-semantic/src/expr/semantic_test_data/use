        ^

//! > ==========================================================================

//! > Test use type enum and const shadowing

//! > test_runner_name
test_expr_semantics(expect_diagnostics: true)

//! > module_code
mod X {
    enum R {
        A: u8,
        B: u16,
    }
}

mod R {
    pub const C: u8 = 2;
}

//! > function_body

//! > expr_code
{ 
    use X::R;
    let _y = R::C;
    let _x = R::B(3); 
}

//! > expected_semantics
Block(
    ExprBlock {
        statements: [
            Item(
                StatementItem,
            ),
            Let(
                StatementLet {
                    pattern: Variable(
                        _y,
                    ),
                    expr: Missing(
                        ExprMissing {
                            ty: <missing>,
                        },
                    ),
                },
            ),
            Let(
                StatementLet {
                    pattern: Variable(
                        _x,
                    ),
                    expr: EnumVariantCtor(
                        ExprEnumVariantCtor {
                            variant: R::B,
                            value_expr: Literal(
                                ExprLiteral {
                                    value: 3,
                                    ty: core::integer::u16,
                                },
                            ),
                            ty: test::X::R,
                        },
                    ),
                },
            ),
        ],
        tail: None,
        ty: (),
    },
)

//! > expected_diagnostics
error: Enum "test::X::R" has no variant "C"
 --> lib.cairo:14:17
    let _y = R::C;
                ^

