//! > ==========================================================================

//! > Test use type extern usage

//! > test_runner_name
test_expr_semantics(expect_diagnostics: false)

//! > module_code
mod X {
    extern type R;
    extern fn new_r() -> R nopanic;
}
use X::{R as RR, new_r};

//! > function_body

//! > expr_code
{ 
    let _y: RR = new_r();
    use X::R;
    let _x: R = new_r();
}

//! > expected_semantics
Block(
    ExprBlock {
        statements: [
            Let(
                StatementLet {
                    pattern: Variable(
                        _y,
                    ),
                    expr: FunctionCall(
                        ExprFunctionCall {
                            function: test::X::new_r,
                            args: [],
                            coupon_arg: None,
                            ty: test::X::R,
                        },
                    ),
                },
            ),
            Item(
                StatementItem,
            ),
            Let(
                StatementLet {
                    pattern: Variable(
                        _x,
                    ),
                    expr: FunctionCall(
                        ExprFunctionCall {
                            function: test::X::new_r,
                            args: [],
                            coupon_arg: None,
                            ty: test::X::R,
                        },
                    ),
                },
            ),
        ],
        tail: None,
        ty: (),
    },
)

