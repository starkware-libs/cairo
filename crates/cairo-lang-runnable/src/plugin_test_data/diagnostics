//! > ==========================================================================

//! > Test runnable with result with no serde.

//! > test_runner_name
ExpandRunnableTestRunner(expect_diagnostics: true)

//! > cairo_code
#[runnable]
fn main(a: felt252, b: felt252) -> NoSerde {
    NoSerde { v: a + b }
}

struct NoSerde {
    v: felt252,
}

//! > generated_cairo_code
