//! > Test expansion of a simple starknet contract.

//! > test_function_name
test_expand_contract

//! > cairo_code
trait ITestContract { func test(a: felt) -> felt; }

#[ContractImpl]
impl TestContractImpl of ITestContract { func test(a: felt) -> felt {
    1
}
}

#[contract(TestContractImpl)]
struct TestContract { }

//! > generated_cairo_code
func test(a: felt) -> felt {
    1
}
