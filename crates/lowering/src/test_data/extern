//! > Test extern functio calling.

//! > test_function_name
test_function_lowering

//! > function
func foo(ref a: felt, b: felt) {
    f(a,b);
    g(a,b);
    h(a,b);
    i(a,b);
}

//! > function_name
foo

//! > module_code
extern func f(ref a: felt, b: felt) -> felt;
extern func g(ref a: felt, b: felt) -> (felt,);
extern func h(ref a: felt, b: felt) -> (felt,felt);
extern func i(ref a: felt, b: felt) -> ((felt,),);

