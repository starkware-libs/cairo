//! > Test extern function enum calling.

//! > test_function_name
test_function_lowering

//! > function
func foo(ref a: felt, b: felt) {
  let x = f(a, b);
  match x {
    MyEnum::A(y) => (),
    MyEnum::B(y) => (),
    MyEnum::C(y) => (),
  }
}

//! > function_name
foo

//! > module_code
enum MyEnum {
  A: felt,
  B: (felt,),
  C: (felt, felt),
}
extern func f(ref a: felt, b: felt) -> MyEnum;

