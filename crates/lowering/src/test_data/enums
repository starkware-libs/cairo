test_function_lowering

//! > function
func foo(a: felt) -> MyEnum {
    let x = true;
    MyEnum::B({(); 5})
}

//! > function_name
foo

//! > module_code
enum MyEnum {
  A: (),
  B: felt,
  C: Box::<((), felt)>,
}

