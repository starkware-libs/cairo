//! > ==========================================================================

//! > Test match enum.

//! > test_function_name
test_function_lowering

//! > function
func foo(x: MyEnum) -> felt {
  match x {
    MyEnum::A(inner) => {return 5;},
    MyEnum::B(inner) => {inner},
    MyEnum::C(inner) => {7}
  }
}

//! > function_name
foo

