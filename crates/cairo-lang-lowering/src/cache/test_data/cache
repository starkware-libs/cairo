//! > Cache Trait Function with Self Impl

//! > test_runner_name
test_cache_check

//! > function
fn foo() -> i32 {
    MyTrait::foo()
}

//! > function_name
foo

//! > module_code
trait MyTrait {
    fn foo() -> i32 {
        let y = |x| x;
        y(3)
    }
}

impl MyImpl of MyTrait {}

