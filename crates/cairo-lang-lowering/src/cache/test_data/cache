//! > ==========================================================================

//! > Cache Impl Impl

//! > test_runner_name
test_cache_check

//! > function
fn foo() {
    bar::<MyOuter>();
}

fn bar<impl I: Outer>() {
    I::Impl::foo();
}

//! > function_name
foo

//! > module_code
trait Inner {
    fn foo();
}

trait Outer {
    impl Impl: Inner;
}

impl MyInner of Inner {
    fn foo() {}
}

impl MyOuter of Outer {}

//! > semantic_diagnostics

//! > lowering_diagnostics

//! > lowering_flat
Parameters:
blk0 (root):
Statements:
End:
  Return()

