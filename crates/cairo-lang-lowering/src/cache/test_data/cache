//! > ==========================================================================

//! > Cache Trait Function with Loop

//! > test_runner_name
test_cache_check

//! > function
fn foo() -> i32 {
    MyTrait::foo()
}

//! > function_name
foo

//! > module_code
trait MyTrait {
    fn foo() -> i32 {
        loop {
            if 6_i32 > 7 {
                break 5;
            }
        }
    }
}

impl MyImpl of MyTrait {}

