//! > ==========================================================================

//! > Test handling cache of macro expansion with expose! calls in items.

//! > test_runner_name
test_cache_check

//! > function
fn foo() {
    bar();
}

//! > function_name
foo

//! > module_code
macro define_bar {
    () => {
        expose! (
            fn bar() {}
        );
    };
}
define_bar!();

