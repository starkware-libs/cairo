//! > ==========================================================================

//! > Test match on libfunc call that uses the refs of the libfunc in one of the arms.

//! > test_runner_name
test_function_lowering

//! > function
fn foo(mut arr: Array::<felt252>, mut b: (felt252,)) -> Array::<felt252> {
    let y = match f(ref arr, ref b) {
        MyEnum::A(x) => arr
    };
    y
}

