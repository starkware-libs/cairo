  (v3: core::felt252) <- test::MyStructMyTrait::<9>::bar(v2)
  (v4: test::MyStruct::<9>, v5: @test::MyStruct::<9>) <- snapshot(v1)
  (v6: core::felt252) <- test::MyStructMyTrait::<9>::bar(v5)
