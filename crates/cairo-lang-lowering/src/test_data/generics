//! > Test extern function calling.

//! > test_function_name
test_function_lowering

//! > function
fn foo() {
    5.foo(true);
}

//! > function_name
foo

//! > module_code
trait MyTrait<T> {
    fn foo<S>(self: T, y: S) -> (T, S);
}
impl MyImpl of MyTrait::<felt> {
    #[inline(always)]
    fn foo<S>(self: felt, y: S) -> (felt, S) {
        (self, y)
    }
}

