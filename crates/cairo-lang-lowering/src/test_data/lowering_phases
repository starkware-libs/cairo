//! > function
fn foo() {
  match gas::withdraw_gas() {
    Option::Some(_) => {},
    Option::None(_) => {
        let mut data = ArrayTrait::new();
        data.append('Out of gas');
        panic(data);
    },
  }
  foo()
}

//! > function_name
foo

