//! > ==========================================================================

//! > Test match on enum tuples.

//! > test_runner_name
test_function_lowering(expect_diagnostics: false)

//! > function
fn foo(a: A, b: A) -> felt252 {
    let x = (@a, b);
    let y = @x;
    match y {
        (A::Two((t, _)), A::One) => **t + 3,
        (A::Two, _) => 2,
        (A::One(_), A::One(_)) => 1,
        (_, A::Three(_)) => 3,
        (_, _) => 6,
    }
}

