//! > ==========================================================================

//! > Test match on enum tuples.

//! > test_runner_name
test_function_lowering(expect_diagnostics: false)

//! > function
fn foo(a: A, b: A) -> felt252 {
    let x = (@a, b);
    let y = @x;
    match y {
        (A::Two((t, _)), A::One(_)) => **t + 3,
        (A::Two(_), _) => 2,
        (A::One(_), A::One(_)) => 1,
        (_, A::Three) => 3,
        (_, _) => 6,
    }
}

//! > function_name
foo

//! > module_code
#[derive(Copy, Drop)]
enum A {
    One: felt252,
    Two: (felt252, felt252),
    Three: (),
}

//! > semantic_diagnostics

//! > lowering_diagnostics

//! > lowering_flat
Parameters: v0: test::A, v1: test::A
blk0 (root):
