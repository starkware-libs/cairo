//! > function
fn foo(a: A, b: A) -> felt252 {
    match (a, b) {
        (A::Two, A::One) => 5,
        (A::Two, _) => 2,
        (A::One(_), A::One(_)) => 1,
        (_, A::Three(_)) => 3,
        (_, A::Four) => 4,
    }
}

