//! > ==========================================================================

//! > Test arm pattern destructure for match-extern (with refs and implicits).

//! > test_runner_name
test_function_lowering

//! > function
fn foo(mut z: felt) {
    match bar(ref z) {
        MyEnum::a((x, (y, z))) => {},
        MyEnum::b((x, y)) => {},
        MyEnum::c(x) => {},
        MyEnum::d((x, y)) => {},
        MyEnum::e(x) => {},
        MyEnum::f((x,)) => {},
        MyEnum::g(x) => {},
        MyEnum::h(x) => {},
    }
}

//! > function_name
