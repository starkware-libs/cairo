//! > ==========================================================================

//! > Test arm pattern destructure for match-enum variable (with refs and implicits).

//! > test_runner_name
test_function_lowering

//! > function
fn foo(mut z: felt) {
    let y = bar(ref z);
    match y {
        MyEnum::a((x, (y, z))) => {},
        MyEnum::b((x, y)) => {},
        MyEnum::c(x) => {},
        MyEnum::d((x, y)) => {},
        MyEnum::e(x) => {},
        MyEnum::f((x,)) => {},
        MyEnum::g(x) => {},
        MyEnum::h(x) => {},
    }
}

//! > function_name
foo

//! > module_code
enum MyEnum {
  a: (felt, (felt, felt)),
  b: (felt, (felt, felt)),
  c: (felt, (felt, felt)),
  d: (felt, felt),
  e: (felt, felt),
  f: (felt,),
  g: (felt,),
  h: felt
}
extern fn bar(ref r: felt) -> MyEnum implicits(RangeCheck) nopanic;

