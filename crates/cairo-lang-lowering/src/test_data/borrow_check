//! > Borrow checking with panic

//! > test_runner_name
test_function_lowering

//! > function
fn foo(ref x: ADrop, y: ADrop) {
  use_a_drop(x);
  bar();
  x = y;
}

fn bar(){
  let mut data = ArrayTrait::new();
  data.append(1);
  panic(data);
}

