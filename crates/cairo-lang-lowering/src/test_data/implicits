//! > Test implicits with multiple jumps to arm blocks.

//! > test_runner_name
test_function_lowering

//! > function
fn foo(a: u256) -> u64 {
    U256TryIntoU64::try_into(a).unwrap()
}

//! > function_name
foo

//! > module_code
impl U256TryIntoU64 of TryInto<u256, u64> {
    #[inline(always)]
    fn try_into(self: u256) -> Option<u64> {
        if (self.high == 0) {
            self.low.try_into()
        } else {
            Option::None
        }
    }
}

