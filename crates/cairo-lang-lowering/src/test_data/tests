blk0 (root):
Statements:
End:
  Match(match_enum(v0) {
    MyEnum::A(v1) => blk1,
    MyEnum::B(v2) => blk2,
    MyEnum::C(v3) => blk3,
  })

blk1:
Statements:
  (v4: core::felt252) <- 5
End:
  Return(v4)

blk2:
Statements:
End:
  Return(v2)

blk3:
Statements:
  (v5: ((), core::felt252)) <- core::box::unbox::<((), core::felt252)>(v3)
  (v6: core::felt252) <- 7
End:
  Return(v6)

