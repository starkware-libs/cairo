//! > ==========================================================================

//! > Test compound expressions.

//! > test_runner_name
test_function_lowering

//! > function
fn foo(a: felt252) -> felt252 {
    let x = {
        7;
    };
    {
        x;
    }
    let _y = if 1 == 1 {
        6
    } else {
        7
    };
    foo(3) + 5 * {
        3;
        a + 0
    }
}

