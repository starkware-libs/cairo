//! > ==========================================================================

//! > Test match enum.

//! > test_runner_name
test_function_lowering

//! > function
fn foo(x: MyEnum) -> felt252 {
  match x {
    MyEnum::A(inner) => {return 5;},
    MyEnum::B(inner) => {inner},
    MyEnum::C(inner) => {
      // A workaround to drop inner.
      let (a, b) = inner.unbox();
      7
    }
  }
}

//! > function_name
foo

//! > module_code
enum MyEnum {
  A: (),
  B: felt252,
  C: Box::<((), felt252)>,
}

