//! > Test fixed size array.

//! > test_runner_name
test_function_lowering(expect_diagnostics: false)

//! > function
fn foo() {
    let x = [10, 20, 30];
    bar(x);
}

//! > function_name
foo

//! > module_code
#[inline(never)]
fn bar(x: [felt252; 3]) {
    let _y = x;
}

