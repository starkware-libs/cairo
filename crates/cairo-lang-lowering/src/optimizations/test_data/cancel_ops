//! > ==========================================================================

//! > Test destructure + construct to different type (optimization not possible).

//! > test_runner_name
test_cancel_ops

//! > function
fn foo(a: (u32,) ) -> MyStruct {
  let (b, ) = a;
  MyStruct { b }
}

//! > function_name
foo

//! > module_code
struct MyStruct {
  b: u32,
}

