//! > Test match optimizer simple scenario.

//! > test_runner_name
test_match_optimizer

//! > function
fn foo() -> Option<u16> {
    let v = get_option();
    match v {
        Option::Some(_) => Option::Some(1_u16),
        Option::None => Option::None,
    }
}

