//! > ==========================================================================

//! > Test skipping of match optimization.

//! > test_runner_name
test_match_optimizer

//! > function
fn foo(a: felt252) -> Option<u16> {
    let v = get_option();

    // The following instruction should block the optimization.
    core::internal::revoke_ap_tracking();
    let v1 = match v {
        Some(_) => Some(1_u16),
        None => None,
    };
    match v1 {
        // v1 is used after the match.
        Some(_) => v1,
        None => Some(2_u16),
    }
}

//! > function_name
foo

