//! > ==========================================================================

//! > Test two matches.

//! > test_runner_name
test_match_optimizer

//! > function
fn foo() -> felt252 {
   let opt = get_option();
   match opt {
      Option::Some(x) => Option::Some(x + 3),
      Option::None(()) => Option::None(())
   }.unwrap()
}

