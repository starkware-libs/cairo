  (v5: core::integer::u128) <- 1u
  (v6: core::integer::u128, v7: @core::integer::u128) <- snapshot(v5)
End:
  Goto(blk7, {})

blk3:
Statements:
End:
  Goto(blk12, {})

blk4:
Statements:
End:
  Goto(blk5, {})

