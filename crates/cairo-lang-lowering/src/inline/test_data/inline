  () <- match_enum(v5) {
    PanicResult::Ok => blk1,
    PanicResult::Err => blk2,
  }
  (v8: core::PanicResult::<core::integer::u128>) <- PanicResult::Ok(v3)
End:
  Return(v4, v8)

