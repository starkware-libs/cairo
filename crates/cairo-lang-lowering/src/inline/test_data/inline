Inputs: v0: core::option::Option::<core::felt>
Statements:
  (v1: core::felt) <- 2u
  () <- match_enum(v0) {
    Option::Some => blk2,
    Option::None => blk3,
  }
End:
  Fallthrough(blk1, {v3 -> v2})

blk1:
Inputs:
Statements:
End:
  Return(v2)

blk2:
Inputs: v4: core::felt
Statements:
End:
  Callsite({v4 -> v3})

blk3:
Inputs: v5: ()
Statements:
  () <- struct_destructure(v5)
End:
  Callsite({v1 -> v3})

