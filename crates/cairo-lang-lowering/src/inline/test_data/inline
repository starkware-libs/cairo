  (v1: core::felt) <- 2u
  (v2: core::felt) <- test::bar::<core::felt>(v0, v1)
End:
  Return(v2)

//! > after
blk0 (root):
Inputs: v0: core::option::Option::<core::felt>
Statements:
  (v1: core::felt) <- 2u
  () <- match_enum(v0) {
    Option::Some => blk2,
    Option::None => blk3,
  }
End:
  Fallthrough(1, {})

