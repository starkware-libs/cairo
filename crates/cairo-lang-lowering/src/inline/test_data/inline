/// If `val` is `Option::Some(x)`, returns `x`. Otherwise, panics.
#[inline]
fn bar<T, impl TDrop: Drop::<T>>(val: Option::<T>, val2: T) -> T {
    match val {
        Option::Some(x) => x,
        Option::None => val2,
    }
}

