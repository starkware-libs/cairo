//! > ==========================================================================

//! > Numeric match with tuples and bindings

//! > test_runner_name
test_create_graph(expect_diagnostics: false)

//! > function_code
fn foo(x: felt252, y: felt252) -> felt252 {
    match (x, y) {
        (1, 3) => 0,
        (1, 4) | (2, 3) => 1,
        (1, 5) => 2,
        (3, _) => 3,
        (x, 0) => x,
        _ => 4,
    }
}

