//! > function_code
fn foo(mut x: felt252) -> felt252 {
    match bar(ref x) {
        MyEnum::Pair1(a) => {
            let (b, _c) = a;
            b.into() + x
        },
        MyEnum::Pair2((_, c)) => { c.into() + x },
        MyEnum::Struct(MyStruct { c, .. }) => { c.into() + x },
        MyEnum::Single(a) => { a.into() + x },
        MyEnum::Opt(Some(a)) => { a.into() + x },
        _ => x + 1,
    }
}

