//! > ==========================================================================

//! > Test closure with copy.

//! > test_runner_name
test_generated_function

//! > function
fn foo(a: u32) {
    let c =  || a;
    identity(c);
    identity(c);
}

//! > function_name
foo

//! > module_code
#[inline(never)]
fn identity<T>(t: T) -> T {
    t
}

