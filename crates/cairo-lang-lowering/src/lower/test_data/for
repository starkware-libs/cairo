//! > function
fn foo() {
    let v = 1_u32;
    v.foo()
}

//! > function_name
foo

//! > module_code
trait MyTrait<T, +Drop<T>, +Copy<T>, +Add<T>> {
    #[inline(never)]
    fn bar(self: T) -> T {
        self
    }

    fn foo(self: T) {
        for _ in 0_u32..5 {
            Self::bar(self) + Self::bar(self);
        }
    }
}

fn bar<T, +Drop<T>, +Copy<T>, +Add<T>>(v: T) {
    let v = 1_u32;
    v.foo()
}

impl MyImpl of MyTrait<u32> {}

