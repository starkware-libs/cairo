//! > ==========================================================================

//! > Test specialization with recursion.

//! > test_runner_name
test_specialized_function

//! > function_code
fn foo(b: felt252) {
    bar1(false, 100000);
}

//! > function_name
foo

//! > module_code
fn bar1(keep: bool, a: felt252) -> felt252 {
    if !keep || a == 0 {
        0
    } else {
        bar2(keep, a - 1)
    }
}

fn bar2(keep: bool, a: felt252) -> felt252 {
    if !keep || a == 0 {
        0
    } else {
        bar1(keep, a - 1)
    }
}

