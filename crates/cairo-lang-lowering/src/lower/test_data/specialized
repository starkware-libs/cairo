//! > ==========================================================================

//! > Test nested tuple inside struct.

//! > test_runner_name
test_specialized_function

//! > function_code
fn foo(x: felt252) {
    bar(false, S { t: (1, x), a: 3 })
}

//! > function_name
foo

//! > module_code
#[derive(Drop)]
struct S {
    t: (felt252, felt252),
    a: felt252,
}

fn bar(keep: bool, s: S) {
    if keep {
        bar_ext(s);
    }
}

