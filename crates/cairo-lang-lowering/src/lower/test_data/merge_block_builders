//! > ==========================================================================

//! > Structs

//! > test_runner_name
test_merge_block_builders

//! > variables
x: B

//! > block_definitions
(
    (x.a, 3), // x.a is packed in the first block.
    (x.m, 0), // Same in all blocks.
    (x.n, 1), // Different.
);
(
    (x.a.m, 4), // x.a is packed in the first block.
    (x.a.n, 5),
    (x.m, 0), // Same in all blocks.
    (x.n, 2), // Different.
);
(
    (x.a.m, 4), // x.a is packed in the first block.
    (x.a.n, 5),
    (x.m, 0), // Same in all blocks.
    (x.n, 2), // Different.
);

//! > module_code
struct A {
    m: felt252,
    n: felt252,
}

struct B {
    a: A,
    m: felt252,
    n: felt252,
}

