test_generated_function

//! > function
fn foo() {
    let a = A { b: B { c: 3 } };
    loop {
        let _x = a.b;
        ex1(_x);
        loop {
            let _y = @a.b.c;
            ex(_y);
            break;
        }
        break;
    };
}

//! > function_name
foo

//! > module_code
extern fn ex(a: @u32) nopanic;
extern fn ex1(a: B) nopanic;
#[derive(Drop, Copy)]
struct B {
    c: u32,
}
#[derive(Drop)]
struct A {
    b: B,
}

