//! > ==========================================================================

//! > Test loop with member path borrowing.

//! > test_runner_name
test_generated_function

//! > function
fn foo(mut a: A, ref b: A) {
    let c = 5_usize;
    loop {
        let mut h = 0;
        a.b.c = c;
        if true {
            h = 1;
            let mut x = 4;
            x = 5;
            b = A { b: a.b, x };
            break b;
        };
    };
}

//! > function_name
foo

//! > module_code
#[derive(Copy, Drop)]
struct A {
    b: B,
    x: felt252,
}
#[derive(Copy, Drop)]
struct B {
    c: usize,
    y: u128,
}

