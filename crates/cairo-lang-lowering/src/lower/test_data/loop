//! > ==========================================================================

//! > Change in block merge in loop.

//! > test_runner_name
test_generated_function

//! > function
fn foo() {
  let mut x = A { a: 3, b: 4 };
  loop {
      if (true) {
        x.a = 0;
      } else {
        x.a = 0;
      }

      if x.a == 0 {
          break;
      };
  };
}

//! > function_name
foo

//! > module_code
#[derive(Copy, Drop, Debug)]
struct A {
  a: u8,
  b: u8,
}

