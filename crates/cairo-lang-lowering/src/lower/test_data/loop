//! > ==========================================================================

//! > Test default implementation with loop.

//! > test_runner_name
test_generated_function

//! > function
fn foo() {
    MyTrait::impl_in_trait();
}

//! > function_name
foo

//! > module_code
trait MyTrait {
    fn impl_in_impl(x: u8) -> bool;
    fn impl_in_trait() -> u8 {
        let mut i = 0;
        loop {
            if Self::impl_in_impl(i) {
                break;
            }
            i += 1;
        }
        i
    }
}

impl MyImpl of MyTrait {
    fn impl_in_impl(x: u8) -> bool {
        x == 30
    }
}

