//! > ==========================================================================

//! > Test early return within a loop.

//! > test_runner_name
test_generated_function

//! > function
fn foo() -> u32 {
    let mut x = 5;
    loop {
        let y = x + 1;
        x = y;
        if y == 10 {
            break true;
        }

        if y == 5 {
            return 5;
        }
    }
    1
}

//! > function_name
foo

//! > module_code
trait MyTrait {
    fn impl_in_impl(x: u8) -> bool;
    fn impl_in_trait() -> u8 {
        let mut i = 0;
        loop {
            if Self::impl_in_impl(i) {
                break;
            }
            i += 1;
        }
        i
    }
}

impl MyImpl of MyTrait {
    fn impl_in_impl(x: u8) -> bool {
        x == 30
    }
}

