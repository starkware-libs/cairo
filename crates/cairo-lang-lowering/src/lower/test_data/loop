//! > ==========================================================================

//! > Change in block merge in loop.

//! > test_runner_name
test_generated_function

//! > function
fn foo() {
    let mut x = A { a: 3, b: 4 };
    loop {
        if (true) {
            x.a = 0;
        } else {
            x.a = 0;
        }

        if x.a == 0 {
            break;
        };
    };
}

