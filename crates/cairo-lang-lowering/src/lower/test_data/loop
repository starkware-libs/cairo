//! > ==========================================================================

//! > Test loop with continue.

//! > test_runner_name
test_generated_function

//! > function
fn foo() -> bool {
    let mut x = 5;
    loop {
        let y = x + 1;
        x = y;
        if y == 7 {
            continue;
        }
        if y == 10 {
            break true;
        };
    }
}

