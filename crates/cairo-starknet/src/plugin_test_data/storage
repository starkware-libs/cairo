//! > Test expansion of StarkNet storage logic.

//! > test_function_name
test_expand_contract

//! > cairo_code
// TODO(ilya): update format.
#[contract]
mod TestContract {
    struct Storage {
        storage_var: felt,
    }
}

//! > generated_cairo_code
mod storage_var {
    fn read() -> felt {
        starknet::storage_read_syscall(
            starknet::storage_address_const::<0x37fe6583f0b27bf0827d87f21c448b20f8709dfc8e4efbdddb75a0b443df714>()
        )
    }
    fn write(value: felt) -> Result::<(), felt> {
        starknet::storage_write_syscall(
            starknet::storage_address_const::<0x37fe6583f0b27bf0827d87f21c448b20f8709dfc8e4efbdddb75a0b443df714>(),
            value
        )
    }
}
#[generated_contract]
mod TestContract {
    struct Storage { storage_var: felt, }
    trait __abi {
    }
    mod __external {
    }
}

