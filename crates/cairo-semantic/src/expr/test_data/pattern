//! > pattern type for a param.

//! > test_function_name
test_function_diagnostics

//! > function
fn foo(x: (felt, felt)) {
}

//! > function_name
foo

//! > module_code

//! > expected_diagnostics

//! > ==========================================================================

//! > Destructure a struct.

//! > test_function_name
test_function_diagnostics

//! > function
fn foo(s2: Struct2) {
    let Struct2{member1, member2, member3} = s2;
    let Struct2{member1, member2} = s2;
    let Struct2{member1, member2, ..} = s2;
    let Struct2{member1, member2, member3: Struct1{member1: a,..}} = s2;
    let Struct2{member1, member2, member3, member1, } = s2;
}

//! > function_name
foo

//! > module_code
struct Struct1 {
    member1: felt,
    member2: (),
}
struct Struct2 {
    member1: felt,
    member2: (),
    member3: Struct1,
}

//! > expected_diagnostics
